    <Or>
      <Class name="hudson.scm.CvsProjectset"/>
      <Class name="hudson.scm.CvsRepositoryItem"/>
    </Or>
  </Match>
  <Match>
    <Bug pattern="DM_DEFAULT_ENCODING"/>
    <Or>
      <Class name="hudson.scm.CvsTagsParamDefinition"/>
