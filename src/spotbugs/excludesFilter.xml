    <Class name="hudson.scm.AbstractCvs"/>
    <Method name="checkout"/>
  </Match>
  <Match>
    <Bug pattern="NP_NULL_ON_SOME_PATH_FROM_RETURN_VALUE"/>
    <Class name="hudson.scm.AbstractCvs"/>
    <Or>
      <Method name="checkout"/>
      <Method name="compareRemoteRevisionWith"/>
